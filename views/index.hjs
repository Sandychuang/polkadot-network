        <div class="content-wrapper">
            <canvas id="dots" height="50" width="50" style="height: 100%; width: 100%;"></canvas>

            <div class="content">
                <div class="container">
                    <div class="shadow"></div>
                    <div class="wrapper">
                        <form action="/signup" class="form" method="post" id="leadGenForm">
                            <h1>Polkadot</h1>
                            <div class="spacer-small"></div>
                            <h2>Keep Posted</h2>

                            <div class="input">
                                <input type="text" placeholder="Enter your e-mail address..." id="email" name="email"/>
                                <span class="border"></span>
                                <span class="error-message">Error! You have entered an invalid email</span>
                                <span class="limiter-message">Too many requests, please try again later</span>
                                <span class="info-message">You will be notified</span>
                            </div>
                            <div class="button">
                                <button type="submit" class="btn">send</button>
                            </div>
                        </form>

                        <div class="navigation">
                            <ul>
